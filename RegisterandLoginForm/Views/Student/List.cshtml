@model IEnumerable<Student>

<table>
    <thead>
        <tr>
            <th>Student ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Qualifications</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in Model)
        {
            <tr>
                <td>@s.StudentId</td>
                <td>@s.FirstName @s.LastName</td>
                <td>@s.Email</td>
                <td>@s.PhoneNumber</td>
                <td>
                    <ul>
                        @foreach (var q in s.Qualifications)
                        {
                            <li>@q.CourseName - @q.University - @q.YearOfPassing - @q.Percentage%</li>
                        }
                    </ul>
                </td>
            </tr>
        }
    </tbody>
</table>